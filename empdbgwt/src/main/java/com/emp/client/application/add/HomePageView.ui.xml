<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
    <g:Label>Enter employee's id</g:Label>
    <g:TextBox ui:field="id"></g:TextBox>
    
    <g:Label>Enter employee's name</g:Label>
    <g:TextBox ui:field="name"></g:TextBox>
    
    <g:Label>Enter employee's age</g:Label>
    <g:TextBox ui:field="age"></g:TextBox>
    
    <g:Label>Enter employee's phone-no.</g:Label>
    <g:TextBox ui:field="phone"></g:TextBox>
    
    <g:Label>Enter employee's address</g:Label>
    <g:TextBox ui:field="address"></g:TextBox> 
    
    <g:Button ui:field="butn">Submit</g:Button>
    
    <g:Hyperlink targetHistoryToken="menu" >go to menu</g:Hyperlink>
    
    </g:HTMLPanel>
</ui:UiBinder>
