<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<h1>Details of particular employees</h1>
		<hr />
		<g:Label ui:field="resultLable"></g:Label>
		<g:FlexTable ui:field="empdetails" title="Employee Details"
			cellSpacing="1" cellPadding="1" borderWidth="1" width="1">
		</g:FlexTable>

        <g:Hyperlink targetHistoryToken="list">go to previous list</g:Hyperlink>
		<g:Hyperlink targetHistoryToken="menu">go to menu</g:Hyperlink>
		

	</g:HTMLPanel>
</ui:UiBinder>
